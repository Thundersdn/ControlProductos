<div class="page-header">
  <%= link_to usuarios_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_usuario_path(@usuario), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show usuario</h1>
</div>

<dl class="dl-horizontal">
  <dt>Rut:</dt>
  <dd><%= @usuario.rut %></dd>

  <dt>Nombre:</dt>
  <dd><%= @usuario.nombre %></dd>

  <dt>Cargo:</dt>
  <dd><%= @usuario.cargo %></dd>

</dl>
